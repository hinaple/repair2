{
    "name": "repair2",
    "version": "2.3.4",
    "description": "REPAIR v2",
    "main": "./out/main/index.js",
    "type": "module",
    "author": "beyondspace.com",
    "homepage": "https://beyond-space.kr",
    "scripts": {
        "format": "prettier --plugin prettier-plugin-svelte --write .",
        "start": "electron-vite preview",
        "dev:editor": "vite --config editor.vite.config.mjs --force",
        "dev:play": "vite --config play.vite.config.mjs --force",
        "dev": "npx concurrently --kill-others \"electron-vite dev --inspect=9229\" \"npm run dev:play\" \"npm run dev:editor\"",
        "build:main": "electron-vite build",
        "build:editor": "vite build --config editor.vite.config.mjs",
        "build:play": "vite build --config play.vite.config.mjs",
        "build": "npm run build:main && npm run build:editor && npm run build:play",
        "postinstall": "electron-builder install-app-deps",
        "build:unpack": "npm run build && electron-builder --dir",
        "build:win": "npm run build && electron-builder --win",
        "build:mac": "npm run build && electron-builder --mac",
        "build:linux": "npm run build && electron-builder --linux"
    },
    "dependencies": {
        "@electron-toolkit/utils": "^4.0.0",
        "adm-zip": "^0.5.16",
        "fs-extra": "^11.3.0",
        "lit": "^3.2.1",
        "serialport": "^13.0.0",
        "socket.io-client": "^4.8.1"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/generator": "^7.26.10",
        "@babel/parser": "^7.26.10",
        "@babel/traverse": "^7.26.10",
        "@sveltejs/vite-plugin-svelte": "^5.0.3",
        "concurrently": "^9.1.2",
        "electron": "^34.2.0",
        "electron-builder": "^25.1.8",
        "electron-vite": "^3.0.0",
        "prettier": "^3.5.1",
        "prettier-plugin-svelte": "^3.3.3",
        "svelte": "^5.20.1",
        "vite": "^6.1.0",
        "vite-plugin-electron-renderer": "^0.14.6"
    }
}
